AAA,CAAA;AAAA,eAAA,CAAA,CAAA,CAAA,IAAA,kBAAA;AAAA,OAAA;AA5BA,KAAA;AA8BA,GAhCA;AAiCA,EAAA,OAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,IAAA;AACA;AAHA,GAjCA;AAsCA,EAAA,QAAA,EAAA;AACA,IAAA,eADA,6BACA;AAAA;;AACA,aAAA;AAAA,eACA,KAAA,CAAA,IAAA,CAAA,QAAA,KAAA,KAAA,CAAA,eAAA,IAAA,qBADA;AAAA,OAAA;AAEA;AAJA;AAtCA,CAAA","sourcesContent":["<template>\n  <v-form\n    ref=\"form\"\n    class=\"d-flex justify-center align-center\"\n    style=\"height: 100vh\"\n    lazy-validation\n    @submit.prevent=\"login()\"\n  >\n    <v-card\n      max-width=\"600\"\n      class=\"pa-2 glassmorphism mx-3\"\n      elevation=\"9\"\n      rounded=\"lg\"\n      color=\"transparent\"\n    >\n      <v-card-title>\n        <h2 class=\"mx-auto\">Form Register</h2>\n      </v-card-title>\n      <v-card-text>\n        <v-row>\n          <v-col md=\"6\" sm=\"12\" xs=\"12\">\n            <v-text-field\n              prepend-icon=\"mdi-face\"\n              label=\"First name\"\n              v-model=\"firstName\"\n              :rules=\"name\"\n            ></v-text-field>\n          </v-col>\n          <v-col md=\"6\" sm=\"12\" xs=\"12\">\n            <v-text-field\n              prepend-icon=\"mdi-face\"\n              label=\"Last name\"\n              v-model=\"lastName\"\n              :rules=\"name\"\n            ></v-text-field>\n          </v-col>\n        </v-row>\n        <v-text-field\n          label=\"Email\"\n          prepend-icon=\"mdi-email\"\n          type=\"email\"\n          :rules=\"emailRules\"\n          v-model=\"user.email\"\n        ></v-text-field>\n        <v-text-field\n          label=\"Phone number\"\n          prepend-icon=\"mdi-phone\"\n          type=\"text\"\n          :rules=\"phoneRules\"\n          v-model=\"user.phone\"\n        ></v-text-field>\n        <v-text-field\n          label=\"Password\"\n          prepend-icon=\"mdi-key\"\n          :type=\"showPassword ? 'text' : 'password'\"\n          :append-icon=\"showPassword ? 'mdi-eye' : 'mdi-eye-off'\"\n          @click:append=\"showPassword = !showPassword\"\n          :rules=\"passwordRules\"\n          v-model=\"user.password\"\n        ></v-text-field>\n        <v-text-field\n          label=\"Confirm password\"\n          prepend-icon=\"mdi-lock\"\n          type=\"password\"\n          :rules=\"[confirmPasswordRules, passwordConfirm]\"\n          v-model=\"confirmPassword\"\n        ></v-text-field>\n      </v-card-text>\n\n      <v-divider></v-divider>\n\n      <v-card-actions>\n        <v-btn color=\"purple white--text\" type=\"submit\"\n          ><v-icon class=\"mr-1\">mdi-account-circle</v-icon> Register</v-btn\n        >\n        <v-spacer></v-spacer>\n        <v-btn type=\"submit\" color=\"info white--text\" to=\"/login\"\n          ><v-icon class=\"mr-1\">mdi-login</v-icon> Login</v-btn\n        >\n      </v-card-actions>\n    </v-card>\n    <div class=\"box\"></div>\n    <div class=\"box2\"></div>\n    <div class=\"box3\"></div>\n  </v-form>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      firstName: \"\",\n      lastName: \"\",\n      confirmPassword: \"\",\n      user: {\n        username: \"\",\n        email: \"\",\n        password: \"\",\n      },\n      showPassword: false,\n      emailRules: [\n        (v) => !!v || \"E-mail is required\",\n        (v) =>\n          /^(([^<>()[\\]\\\\.,;:\\s@']+(\\.[^<>()\\\\[\\]\\\\.,;:\\s@']+)*)|('.+'))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/.test(\n            v\n          ) || \"E-mail must be valid\",\n      ],\n      passwordRules: [\n        (v) => !!v || \"Password is required\",\n        (v) =>\n          /(?=.*\\d)(?=.*[a-z])(?=.*[A-Z]).{6,}/.test(v) ||\n          \"Password must contain at least lowercase letter, one number, a special character and one uppercase letter\",\n      ],\n      phoneRules: [\n        (v) => !!v || \"Phone number is required\",\n        (v) => v.length < 12 || \"Phone number max 12 digits\",\n      ],\n      confirmPasswordRules: [(v) => !!v || \"Password is required\"],\n      name: [(v) => !!v || \"Name is required\"],\n    };\n  },\n  methods: {\n    login() {\n      this.$emit(\"navbar\", true);\n    },\n  },\n  computed: {\n    passwordConfirm() {\n      return () =>\n        this.user.password === this.confirmPassword || \"Password dont match\";\n    },\n  },\n};\n</script>\n\n<style>\n.box {\n  position: absolute;\n  content: \"\";\n  top: 40%;\n  left: 50%;\n  width: 70px;\n  height: 70px;\n  transform: translate(-50%, 100%);\n  background: rgb(65, 65, 184);\n  border-radius: 50%;\n  backdrop-filter: blur(1px);\n  z-index: 1;\n}\n</style>"],"sourceRoot":"src/components"}]}